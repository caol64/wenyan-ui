  "author": "Lei <caol64@gmail.com> (https://github.com/caol64)",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm prepack",
    "prepack": "svelte-kit sync && svelte-package",
    "prepublishOnly": "pnpm build && publint"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "catalog:",
    "@sveltejs/kit": "catalog:",
    "@sveltejs/package": "catalog:",
    "@sveltejs/vite-plugin-svelte": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "publint": "catalog:",
    "svelte": "catalog:",
    "svelte-check": "catalog:",
    "tailwindcss": "catalog:",
    "vite": "catalog:"
  },
  "peerDependencies": {
    "@codemirror/commands": "catalog:",
    "@codemirror/lang-markdown": "catalog:",
    "@codemirror/state": "catalog:",
    "@codemirror/view": "catalog:",
    "@codemirror/language-data": "catalog:",
    "@fsegurai/codemirror-theme-vscode-dark": "catalog:",
    "@fsegurai/codemirror-theme-vscode-light": "catalog:",
    "@wenyan-md/core": "workspace:^",
    "codemirror": "catalog:",
    "dompurify": "catalog:",
    "svelte": "catalog:",
    "tailwindcss": "catalog:"
  },
  "keywords": [
    "svelte"
  ]
